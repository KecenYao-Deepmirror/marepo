ninja_required_version = 1.3
cxx = c++

cflags = -pthread -B /opt/anaconda/envs/public_marepo/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/opt/anaconda/envs/public_marepo/include/opencv4 -I/opt/anaconda/envs/public_marepo/lib/python3.8/site-packages/torch/include -I/opt/anaconda/envs/public_marepo/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/opt/anaconda/envs/public_marepo/lib/python3.8/site-packages/torch/include/TH -I/opt/anaconda/envs/public_marepo/lib/python3.8/site-packages/torch/include/THC -I/opt/anaconda/envs/public_marepo/include/python3.8 -c
post_cflags = -fopenmp -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=dsacstar -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc





build /workspace/project/marepo/dsacstar/build/temp.linux-x86_64-3.8/dsacstar.o: compile /workspace/project/marepo/dsacstar/dsacstar.cpp
build /workspace/project/marepo/dsacstar/build/temp.linux-x86_64-3.8/thread_rand.o: compile /workspace/project/marepo/dsacstar/thread_rand.cpp







